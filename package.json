{
  "name": "@fal-works/p5-extension",
  "description": "An extension for p5.js.",
  "version": "0.1.9",
  "author": "FAL",
  "bugs": {
    "url": "https://github.com/fal-works/p5-extension/issues"
  },
  "dependencies": {
    "@fal-works/creative-coding-core": "^0.1.10",
    "p5": "^0.9.0"
  },
  "devDependencies": {
    "@types/p5": "^0.9.0",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.19.1",
    "rollup": "^1.26.5",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-typescript2": "^0.25.2",
    "terser": "^4.4.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.7.2"
  },
  "directories": {
    "lib": "lib"
  },
  "eslintIgnore": [
    "*.min.js",
    "*.min.mjs",
    "types/**/*.ts"
  ],
  "files": [
    "lib",
    "types"
  ],
  "homepage": "https://github.com/fal-works/p5-extension#readme",
  "jsnext:main": "lib/p5-extension.mjs",
  "keywords": [],
  "license": "MIT",
  "main": "lib/p5-extension.js",
  "module": "lib/p5-extension.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fal-works/p5-extension.git"
  },
  "scripts": {
    "build": "npm-run-all rollup eslint:es terser:umd eslint:umd terser:es",
    "eslint:es": "eslint --fix ./lib/p5-extension.mjs",
    "eslint:umd": "eslint --fix ./lib/p5-extension.js",
    "lint": "eslint --fix ./src/**/*.ts",
    "rollup": "rollup -c",
    "terser:es": "terser ./lib/p5-extension.mjs -c -m -o ./lib/p5-extension.min.mjs --comments --ecma 6",
    "terser:umd": "terser ./lib/p5-extension.js -c -m -o ./lib/p5-extension.min.js --comments --ecma 6",
    "typedoc": "typedoc src"
  },
  "types": "types/p5-extension.d.ts"
}
